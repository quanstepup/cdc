{
  "name": "main-jdbc-sink-prod",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "5",
    "topics.regex": "sink-db-main-prod.public.(.*)",
    "connection.url": "jdbc:postgresql://192.0.0.0:5432/db_app_tofu_main_prod?user=123&password=123",
    "dialect.name": "PostgreSqlDatabaseDialect",
    "table.name.format": "${topic}",
    "auto.create": "true",
    "insert.mode": "upsert",
    "delete.enabled": "true",
    "schema.evolution": "basic",
    "auto.evolve": "true",
    "pk.mode": "record_key",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "true",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "true",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "([^.]+)\\.([^.]+)\\.([^.]+)",
    "transforms.route.replacement": "$3",
    "mode": "incrementing",
    "incrementing.column.name": "id",
    "transforms": "unwrap, route",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "delete.handling.mode": "none",
    "max.batch.size": "204800",
    "max.queue.size": "819200",
    "max.request.size": "524288000",
    "heartbeat.interval.ms": "5000"
  }
}